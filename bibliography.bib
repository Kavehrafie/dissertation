@article{khakpourDivorceAvantgardesSurrealism2016,
  title = {A Divorce of Avant-Gardes: {{Surrealism}} and Socialism in Post-{{Reza Shah Iran}}},
  author = {Khakpour, Arta},
  date = {2016},
  journaltitle = {Middle Eastern Literatures},
  volume = {19},
  number = {2},
  pages = {119--134},
  doi = {10.1080/1475262X.2016.1199092},
  abstract = {Renato Poggioli’s “divorce of the avant-gardes” was one of the more colorful descriptions of the rift between aesthetic and social radicalism evident in the evolution of the historical avant-garde movements. Outside the geographic domain of studies of the Euro-American avant-garde, however, and even when those studies approach later movements such as Tel Quel, the existence of a natural allegiance between revolutionaries of art and revolutionaries of politics is more often than not taken for granted. This article examines an Iranian manifestation of such an avant-garde domestic dispute that occurred during a famously contested and vibrant period in Iran’s history: the period between the fall of Reza Shah in 1941 and the coup that overthrew Prime Minister Mosaddeq in 1953. A debate was initiated—in the pages of literary journals and in the meeting rooms of literary societies and political parties—that, while not as cataclysmic as the political events of the era, had profound ramifications on Persian literature and what it meant to be a writer in Iran.},
