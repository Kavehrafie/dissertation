@article{moussavi-aghdamArtHistoryNational2014,
  title = {Art {{History}}, '{{National Art}}' and {{Iranian Intellectuals}} in the 1960s},
  author = {Moussavi-Aghdam, Combiz},
  date = {2014},
  journaltitle = {British journal of Middle Eastern studies},
  volume = {41},
