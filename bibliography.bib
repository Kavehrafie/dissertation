@incollection{gellTechnologyEnchantmentEnchantment1999,
  title = {The {{Technology}} of {{Enchantment}} and the {{Enchantment}} of {{Technology}}},
  booktitle = {The {{Art}} of {{Anthropology}}},
  author = {Gell, Alfred},
  date = {1999},
  publisher = {Routledge},
  abstract = {The complaint is commonly heard that art is a neglected topic in present-day social anthropology, especially in Britain. This chapter argues that the anthropology of art can do this by considering art as a component of technology. Art, as a separate kind of technical activity, only carries further, through a kind of involution, the enchantment which is immanent in all kinds of technical activity. The matchstick model, functioning essentially as an advertisement, is part of a technology of enchantment, but it achieves its effect via the enchantment cast by its technical means, the manner of its coming into being, or, rather, the idea which one forms of its coming into being, since making a matchstick model of Salisbury Cathedral may not be as difficult, or as easy, as one imagines. The Trobnand garden is, therefore, both the outcome of a certain system of technical knowledge and at the same time a collective work of art, which produces yams by magic.},
