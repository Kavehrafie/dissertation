@artwork{tanavoliHandPoet1966,
  title = {Hand of a Poet},
  author = {Tanavoli, Parviz},
  date = {1966},
  abstract = {According to [[@fockIranischeModerneBildenden2011]]p,186, this work is almost contemporaneous with Combine Paintings.  Tanavoli made use of cage motif and attached a three-dimensional cage to it. Tanavoli reported that this installation brought criticism from the religious side at the time. It was believed that Tanavoli was making fun of religious acts at hoy shrine. The possible association of someone imprisoned was not expressed towards Tanavoli, as the lattice clearly appeared to come from a religious architectural context. In addition, the hands reach from the outside to the inside, as in an act of prayer, whereas in a prison situation they should have been arranged the other way around. viewers who were not familiar with Tanavoli's subjective iconography would have associated Tanavoli's adminishing attitudes towards the arrests in the 1960s in Iran. Since 1963, Mohammad Reza has strenghtend his autocratic rule through violent methods. The infamous secret police Savak was established in 1957. Tanavoli, however, refused that: {$>$} Political involvement can be a form of inspiration for some artists, but I have never been so involved. Like anyone else, I need an idea to start working on and to develop, but I have always found mine in my imagination. I have never been inspired by an idea that is limited to a particular incident or a particular set of circumstances, and I express no particular 'message' in my work. Tanavoli, Sculptor with Insight, Tehran Journal, 25.05.1968. FORGA , Elizabeth, Sculptor with Insight, 25.02.1968 cited in [[@fockIranischeModerneBildenden2011]] p.193},
  loc = {Gray Art Gallery and Study center},
  medium = {wood, plastic, oil paint},
  size = {121.3 x 82.6 x 8.9 cm},
  file = {/home/tg/Zotero/storage/64DFFQKY/Tanavoli-1966-Hand_of_a_poet.png}
}

