@article{croninNobleRobbersAvengers2016,
  title = {Noble {{Robbers}}, Avengers and Entrepreneurs: {{Eric Hobsbawm}} and {{Banditry}} in {{Iran}}, the {{Middle East}} and {{North Africa}}},
  shorttitle = {Noble {{Robbers}}, Avengers and Entrepreneurs},
  author = {Cronin, Stephanie},
