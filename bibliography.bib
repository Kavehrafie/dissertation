  keywords = {HISTORY / Social History,Philosophy / Political,Political Science / Political Ideologies / Capitalism,Political Science / Public Policy / Cultural Policy,Social Science / Social Classes & Economic Disparity,Social Science / Sociology / Social Theory}
}

