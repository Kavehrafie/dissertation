  pagetotal = {313},
  keywords = {Art / Criticism & Theory,Art / History / Modern (late 19th Century to 1945)},
  file = {/home/tg/Zotero/storage/K9PVLDH5/Foster - 1995 - Compulsive Beauty.pdf}
}

